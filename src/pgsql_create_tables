# SQL scripts to create table in pgsql db
CREATE TABLE freddie_origination (
  credit_score text,
  first_payment_date text,
  first_time_homebuyer_flag char,
  maturity_date text,
  msa text,
  mip text,
  num_of_units text,
  occupancy_status text,
  ori_cltv text,
  ori_dti text,
  ori_upb text,
  ori_ltv text,
  ori_interest_rate text,
  channel text,
  prepayment_penalty_flag char,
  product_type text,
  property_state text,
  property_type text,
  postal_code text,
  loan_seq_no text,
  loan_purpose char,
  original_loan_term text,
  number_of_borrowers text,
  seller_name text,
  servicer_name text,
  super_conforming_flag char
  #,pre_harp_loan_seq_no text
);

CREATE TABLE fannie_origination (
  loan_seq_no text,
  channel text,
  seller_name text,
  original_interest_rate numeric,
  original_upb numeric,
  original_loan_term integer,
  origination_date text,
  first_payment_date text,
  original_ltv numeric,
  original_cltv numeric,
  number_of_borrowers integer,
  dti numeric,
  credit_score integer,
  first_time_homebuyer_indicator text,
  loan_purpose text,
  property_type text,
  number_of_units text,
  occupancy_status text,
  property_state text,
  zip_code text,
  mip numeric,
  product_type text,
  co_borrower_credit_score integer,
  mortgage_insurance_type integer,
  relocation_mortgage_indicator text
);
